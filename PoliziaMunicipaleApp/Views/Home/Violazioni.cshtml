@model IEnumerable<PoliziaMunicipaleApp.Violazione>

<p class="mt-5 fs-5">
    @Html.ActionLink("Inserisci Violazione", "NuovaViolazione")
</p>
<table class="table">
    <tr>

        <th>
            @Html.DisplayNameFor(model => model.Descrizione)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>

        <td>
            @Html.DisplayFor(modelItem => item.Descrizione)
        </td>

    </tr>
}

</table>
