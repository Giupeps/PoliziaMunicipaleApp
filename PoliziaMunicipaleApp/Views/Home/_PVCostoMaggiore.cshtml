@model IEnumerable<PoliziaMunicipaleApp.Verbale>


<table class="table border rounded">
    <h4 class="mb-3">Multe maggiori di 400 euro</h4>
    <tr>

        <th>
            @Html.DisplayNameFor(model => model.IdAnagrafica.Nome)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IdAnagrafica.Cognome)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Idviolazione.Descrizione)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Importo)
        </th>


        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>

        <td>
            @Html.DisplayFor(modelItem => item.IdAnagrafica.Nome)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IdAnagrafica.Cognome)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Idviolazione.Descrizione)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Importo)
        </td>

    </tr>
}

</table>
